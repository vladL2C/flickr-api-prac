<div class="flickr">
  <ul>
    <% photos.each do |row| %>
      <li>
        <% row.each do |p| %>
          <%= link_to(image_tag(FlickRaw.url_s(p), :class => 'flickr_photo', :title => p.title, :border => 0, :size => '75x75'), FlickRaw.url_photopage(p)) %>

        <% end %>
      </li>
    <% end %>
  </ul>
</div>  
